function updateUser(){$.post("/userTools/updateUser",$("#userInfo").serialize(),function(e,s){$.notify({title:"<strong>Success!</strong>",message:e.message,icon:"/images/kip.gif"},{icon_type:"image",type:"success"})}).fail(function(e){$.notify({title:"<strong>Warning!</strong>",icon:"/images/aaron.gif",message:e.responseJSON.message+". Information not updated"},{type:"danger",icon_type:"image"})})}function updatePwd(){$.post("/userTools/updatePwd",$("#updatePwd").serialize(),function(e,s){$.notify({title:"<strong>Success!</strong>",message:e.message,icon:"/images/kip.gif"},{icon_type:"image",type:"success"}),$("#changePwd").modal("hide")}).fail(function(e){console.log(e),$("#warning").html("<strong>Warning!</strong> "+e.responseJSON.message).show()})}$(document).ready(function(){$(".form-control").keyup(function(){$("#save").attr("disabled",!1)})});